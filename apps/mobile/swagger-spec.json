          },
          "403": {
            "description": "Access denied"
          },
          "404": {
            "description": "Journal entry not found"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Update journal entry",
        "tags": [
          "Journal"
        ]
      },
      "delete": {
        "description": "Delete a journal entry",
        "operationId": "JournalController_deleteEntry",
        "parameters": [
          {
            "name": "id",
            "required": true,
