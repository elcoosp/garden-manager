        ],
        "responses": {
          "200": {
            "description": "Journal entry deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeleteResponseDto"
                }
              }
            }
          },
          "403": {
            "description": "Access denied"
          },
          "404": {
            "description": "Journal entry not found"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Delete journal entry",
        "tags": [
          "Journal"
        ]
      }
    }
  },
  "info": {
    "title": "Garden Manager API",
    "description": "API documentation for Garden Manager application",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "Auth",
      "description": "Authentication endpoints"
    },
    {
      "name": "Garden",
      "description": "Garden management endpoints"
    },
    {
      "name": "Journal",
      "description": "Garden journal endpoints"
    }
  ],
  "servers": [],
  "components": {
    "securitySchemes": {
      "JWT-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "name": "JWT",
        "description": "Enter JWT token",
        "in": "header"
      }
    },
    "schemas": {
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password",
          "name"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "email",
          "name",
          "createdAt"
        ]
      },
      "AuthResponse": {
        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "accessToken": {
            "type": "string"
          },
          "refreshToken": {
            "type": "string"
          }
        },
        "required": [
          "user",
          "accessToken",
          "refreshToken"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "ForgotPasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "MessageResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message"
        ]
      },
      "ResetPasswordDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "newPassword": {
            "type": "string"
          }
        },
        "required": [
          "token",
          "newPassword"
        ]
      },
      "CreateGardenPlanDto": {
        "type": "object",
        "properties": {
          "zipCode": {
            "type": "string",
            "description": "ZIP code for location-based recommendations",
            "example": "90210"
          },
          "gardenSize": {
            "type": "string",
            "description": "Size of the garden",
            "enum": [
              "small",
              "medium",
              "large"
            ],
            "example": "medium"
          },
          "sunlightHours": {
            "type": "number",
            "minimum": 0,
            "maximum": 24,
            "description": "Average daily sunlight hours",
            "example": 6
          },
          "experienceLevel": {
            "type": "string",
            "description": "Gardener experience level",
            "enum": [
              "beginner",
              "intermediate",
              "expert"
            ],
            "example": "beginner"
          },
          "goals": {
            "description": "Garden goals and preferences",
            "example": [
              "grow vegetables",
              "attract pollinators"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "zipCode",
          "gardenSize",
          "sunlightHours",
          "experienceLevel",
          "goals"
        ]
      },
      "GardenPlanResponseDto": {
        "type": "object",
        "properties": {
          "gardenId": {
            "type": "string",
            "description": "Unique garden profile ID",
            "example": "a1b2c3d4-e5f6-7890-abcd-ef1234567890"
          },
          "plantingPlan": {
            "type": "object",
            "description": "Generated planting plan with recommendations"
          }
        },
        "required": [
          "gardenId",
          "plantingPlan"
        ]
      },
      "GardenProfileResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "a1b2c3d4-e5f6-7890-abcd-ef1234567890"
          },
          "userId": {
            "type": "string",
            "example": "u9i8o7p6-q5w4-3e2r-1t0y-9u8i7o6p5q4w"
          },
          "zipCode": {
            "type": "string",
            "example": "90210"
          },
          "gardenSize": {
            "type": "string",
            "example": "medium"
          },
          "sunlightHours": {
            "type": "number",
            "example": 6
          },
          "experienceLevel": {
            "type": "string",
            "example": "beginner"
          },
          "goals": {
            "example": [
              "grow vegetables",
              "attract pollinators"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "plantingPlan": {
            "type": "object",
            "nullable": true,
            "description": "Full planting plan object"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "userId",
          "zipCode",
          "gardenSize",
          "sunlightHours",
          "experienceLevel",
          "goals",
          "createdAt",
          "updatedAt"
        ]
      },
      "DiagnosePlantDto": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "description": "Description of plant issue or question",
            "example": "My tomato plant leaves are turning yellow"
          },
          "image": {
            "type": "string",
            "description": "Base64 encoded image or URL",
            "example": "data:image/jpeg;base64,/9j/4AAQSkZJRg..."
          }
        },
        "required": [
          "description"
        ]
      },
      "DiagnosisResponseDto": {
        "type": "object",
        "properties": {
          "diagnosisId": {
            "type": "string",
            "description": "Unique diagnosis ID",
            "example": "d1a2g3n4-o5s6-7i8s-9abc-def123456789"
          },
          "diagnosis": {
            "type": "object",
            "description": "AI-generated diagnosis and recommendations"
          }
        },
        "required": [
          "diagnosisId",
          "diagnosis"
        ]
      },
      "DiagnosisListItemDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "d1a2g3n4-o5s6-7i8s-9abc-def123456789"
          },
          "description": {
            "type": "string",
            "example": "My tomato plant leaves are turning yellow"
          },
          "imageUrl": {
            "type": "string",
            "example": "https://example.com/image.jpg"
          },
          "diagnosis": {
            "type": "object",
            "description": "Diagnosis result object"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "description",
          "diagnosis",
          "createdAt"
        ]
      },
      "CreateJournalEntryDto": {
        "type": "object",
        "properties": {
          "gardenId": {
            "type": "string",
            "format": "uuid",
            "description": "Garden profile ID",
            "example": "a1b2c3d4-e5f6-7890-abcd-ef1234567890"
          },
          "date": {
            "format": "date-time",
            "type": "string",
            "description": "Entry date",
            "example": "2024-03-15T10:30:00Z"
          },
          "notes": {
            "type": "string",
            "description": "Journal entry notes",
            "example": "Planted tomatoes in the north bed. Watered all plants."
          },
          "photoUrl": {
            "type": "string",
            "description": "Photo URL or base64 encoded image",
            "example": "https://example.com/garden-photo.jpg"
          }
        },
        "required": [
          "gardenId",
          "date",
          "notes"
        ]
      },
      "JournalEntryResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "j1o2u3r4-n5a6-7l8e-9ntry-abc123456789"
          },
          "gardenId": {
            "type": "string",
            "example": "a1b2c3d4-e5f6-7890-abcd-ef1234567890"
          },
          "date": {
            "format": "date-time",
            "type": "string",
            "example": "2024-03-15T10:30:00Z"
          },
          "notes": {
            "type": "string",
            "example": "Planted tomatoes in the north bed. Watered all plants."
          },
          "photoUrl": {
            "type": "string",
            "example": "https://example.com/garden-photo.jpg"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "id",
          "gardenId",
          "date",
          "notes",
          "createdAt",
          "updatedAt"
        ]
      },
      "UpdateJournalEntryDto": {
        "type": "object",
        "properties": {
          "date": {
            "format": "date-time",
            "type": "string",
            "description": "Updated entry date",
            "example": "2024-03-15T10:30:00Z"
          },
          "notes": {
            "type": "string",
            "description": "Updated notes",
            "example": "Planted tomatoes and peppers. Watered all plants thoroughly."
          },
          "photoUrl": {
            "type": "string",
            "description": "Updated photo URL",
            "example": "https://example.com/updated-photo.jpg"
          }
        }
      },
      "DeleteResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Success message",
            "example": "Journal entry deleted successfully"
          }
        },
        "required": [
          "message"
        ]
      }
    }
  }
}