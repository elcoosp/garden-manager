              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GardenPlanResponseDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Generate planting plan",
        "tags": [
          "Garden"
        ]
      }
    },
