        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get journal entry by ID",
        "tags": [
          "Journal"
        ]
      },
      "put": {
        "description": "Update an existing journal entry",
        "operationId": "JournalController_updateEntry",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
