          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get journal entries by garden",
        "tags": [
          "Journal"
        ]
      }
    },
    "/journal/{id}": {
      "get": {
        "description": "Retrieve a specific journal entry",
        "operationId": "JournalController_getEntry",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "description": "Journal entry ID",
